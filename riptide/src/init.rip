# This script bootstraps the runtime environment to the standard state.
#
# This script must never throw an exception, or the runtime will panic.

def lang (require lang)

defglobal print $lang->print
defglobal println $lang->println
defglobal echo $lang->println
defglobal exit $lang->exit
defglobal dump $lang->dump
defglobal eq $lang->eq

defglobal eval [string] {
    call $source $string
}
